AT_SETUP([strin])
AT_KEYWORDS([stdstream])

AT_DATA([input],
[Omnis enim res, quae dando non deficit,
dum habetur et non datur, nondum habetur,
quomodo habenda est.
])

AT_CHECK([
strin < input
],
[0],
[Omnis enim res, quae dando non deficit,
dum habetur et non datur, nondum habetur,
quomodo habenda est.
])

AT_CLEANUP


