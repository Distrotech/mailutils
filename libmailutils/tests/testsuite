query[1] <arg 1>
query[2] <arg 2>
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/url.at:771"
$at_failed && at_fn_log_failure
$at_traceon; }

