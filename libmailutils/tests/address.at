AT_BANNER(Email address)

dnl ------------------------------------------------------------
dnl TESTADDR(ADDR, [KW = `'], [STATUS = `0'], [STDOUT = `'],
dnl          [STDERR = `'], [RUN-IF-FAIL], [RUN-IF-PASS])
dnl
m4_define([TESTADDR],[
AT_SETUP([Address: [$1]])
AT_KEYWORDS([address $2])
AT_CHECK([m4_if(m4_index([$1],[']),-1,[addr '[$1]'],
[AT_DATA([input],[$1
])
addr < input])],m4_shift(m4_shift($@)))
AT_CLEANUP
])

