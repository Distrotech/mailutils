AT_SETUP([double-dash with permutation])
AT_KEYWORDS([parseopt parseopt_long parseopt_short parseopt_double_dash parseopt19.at])
AT_CHECK([
parseopt -x more --file=foobar -- -a --optional arg
],
[0],
[rc=0
file_name=foobar
opt_value=initial
x_option=1
a_option=0
find_value=(null)
d_option=0
jobs=0
argv:
0: more
1: -a
2: --optional
3: arg
])
AT_CLEANUP

