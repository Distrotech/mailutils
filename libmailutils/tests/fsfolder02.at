AT_SETUP([Rename])
AT_KEYWORDS([fsfolder folder-rename])

AT_CHECK([
mkdir dir
> dir/foo
fsfolder -name=dir rename foo bar
fsfolder -name=dir list "" "*"
> dir/baz
fsfolder -name=dir rename bar baz
],
[0],
[renaming foo to bar
rename successful
listing '' '*'
d- /    0 dir/bar
renaming bar to baz
],
[fsfolder: mu_folder_rename(bar) failed: File exists
])

AT_CLEANUP

