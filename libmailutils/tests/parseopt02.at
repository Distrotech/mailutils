AT_SETUP([short options])
AT_KEYWORDS([parseopt parseopt_short parseopt02])
AT_CHECK([
PARSEOPT_DEFAULT
parseopt -f file -x -a -d command line arguments
],
[0],
[rc=0
file_name=file
opt_value=initial
x_option=1
a_option=1
find_value=(null)
d_option=1
jobs=0
argv:
0: command
1: line
2: arguments
])
