#AT_START_154
at_fn_group_banner 154 'mhn.at:896' \
  "mhn-compose: external data" "                     " 20
at_xfail=no
(
  $as_echo "154. $at_setup_line: testing $at_desc ..."
  $at_traceon



test -d Mail || mkdir Mail
