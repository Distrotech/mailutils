mkdir Mail/drafts
cat >Mail/drafts/2 <<'_ATEOF'
From: mhtester@example.net
To: gray@example.net
Subject: Draftfolder test

Message body
_ATEOF

echo \"cur: 2\" > Mail/drafts/.mh_sequences

send -draftfolder +drafts || echo \$?
sed 's/: Date: .*/: Date: now/' \$MTA_DIAG
"
