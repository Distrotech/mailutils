  "mhseq: invalid counted range (negative)" "        " 2
at_xfail=no
(
  $as_echo "24. $at_setup_line: testing $at_desc ..."
  $at_traceon



test -d Mail || mkdir Mail
HOME=`pwd`
export HOME
MH=$HOME/mh_profile
export MH
